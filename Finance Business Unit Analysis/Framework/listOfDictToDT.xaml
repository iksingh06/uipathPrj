  <Sequence sap2010:Annotation.AnnotationText="Convert a list&lt;Dictionary&lt;string, object&gt;&gt; into a datatable" DisplayName="listOfDictToDT" sap2010:WorkflowViewState.IdRef="Sequence_1">
